[user]
	name = Simon Weber
	email = simon.weber@venmo.com
[core]
	editor = vim
	autocrlf = input
	excludesfile = ~/.gitignore
[color]
    ui = auto
    branch = auto
    status = auto
    diff = auto
[alias]
	lg = log --oneline --decorate --all --graph -30
    rb = for-each-ref --format='%(refname:short)' --sort=-authordate --count=8 refs/heads/
[push]
	default = simple
